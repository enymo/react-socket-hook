{
  "name": "@enymo/react-socket-hook",
  "version": "1.1.4",
  "description": "React hook for interacting with a socket.io server",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.ts"
      }
    }
  },
  "tsup": {
    "entry": [
      "src/js/index.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsup"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enymo/react-socket-hook.git"
  },
  "author": "enymo GmbH",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.25",
    "postcss": "^8.4.17",
    "tsup": "^8.0.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "socket.io-client": "^4.5.3"
  }
}
